{
    "name": "larabizcms/for-dev",
    "description": "Larabiz CMS package for development",
    "keywords": [
        "PHP",
        "Laravel",
        "CMS",
        "Larabiz",
        "ForDev",
        "Development",
        "Development Tools",
        "LarabizCMS"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "LarabizCMS\\ForDev\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LarabizCMS\\ForDev\\Providers\\ForDevServiceProvider"
            ]
        }
    }
}
